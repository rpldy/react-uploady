{
  "version": "0.7.1",
  "name": "@rpldy/uploader",
  "description": "the processing and queuing engine for react-uploady",
  "author": "yoav niran (https://github.com/yoavniran)",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "main:dev": "src/index.js",
  "exports": {
    ".": "lib/esm/index.js",
    "require": "lib/cjs/index.js",
    "import": "lib/esm/index.js",
    "default": "lib/esm/index.js"
  },
  "types": "types/index.d.ts",
  "license": "MIT",
  "keywords": [
    "upload",
    "file upload",
    "react",
    "hooks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rpldy/react-uploady.git",
    "directory": "packages/uploader"
  },
  "scripts": {
    "build": "node ../../../scripts/build.js",
    "flow:copy": "flow-copy-source ./src ./lib/flow --ignore **/*.test.js --ignore **/*.mock.js; cp src/types.js lib/types.js"
  },
  "dependencies": {
    "@rpldy/life-events": "^0.7.0",
    "@rpldy/sender": "^0.7.0",
    "@rpldy/shared": "^0.7.0",
    "@rpldy/simple-state": "^0.7.0"
  },
  "devDependencies": {
    "flow-bin": "^0.133.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
